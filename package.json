{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "app/main.jsx",
  "scripts": {
    "precommit": "npm run lint",
    "lint": "eslint app/** --cache --ext .jsx,.js --format=node_modules/eslint-formatter-pretty .",
    "test": "npm run lint",
    "build": "webpack",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base app",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack --config webpack.production.config.js",
    "validate": "npm ls",
    "commit": "git cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "auth0-js": "^8.8.0",
    "babel-runtime": "6.x",
    "react": "15.x",
    "react-dom": "15.x"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "6.x",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.x",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-preset-react": "6.x",
    "babel-preset-stage-0": "6.x",
    "bulma": "^0.4.4",
    "commitizen": "^2.9.3",
    "conventional-changelog-cli": "^1.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.4",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^4.3.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "font-awesome-webpack": "0.0.5-beta.2",
    "gapi": "0.0.3",
    "leaflet": "^1.1.0",
    "leaflet-react": "^3.0.5",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "node-sass": "^4.5.3",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^1.4.0",
    "react-leaflet": "^1.4.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "keywords": [
    "es6",
    "web app",
    "boilerplate"
  ],
  "author": "Ruan Yifeng",
  "license": "MIT",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
